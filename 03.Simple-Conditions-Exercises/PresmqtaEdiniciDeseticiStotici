using System;

namespace demo7
{
    class Program
    {
        static void Main(string[] args)
        {
            long number = long.Parse(Console.ReadLine());

            Console.WriteLine("edinici: {0}" , + number % 10);
            Console.WriteLine("desetici: {0}", + (number/10) % 10);
            Console.WriteLine("stotici: {0}", + (number / 100) % 10);
        }
    }
}
