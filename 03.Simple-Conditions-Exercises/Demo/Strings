//Сравняване на стринг

using System;

namespace demo3
{
    class testSravnqvane
    {
        static void Main(string[] args)
        {
            string text = Console.ReadLine();
            if (text == "pesho")
            {
                Console.WriteLine("Tova e pesho");
            }
            else
            {
                Console.WriteLine("tova NE e pesho");
            }
        }
    }
}
