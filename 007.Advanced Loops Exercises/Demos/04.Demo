using System;

namespace _08.Demo
{
    class Program
    {
        static void Main(string[] args)
        {
            // Syshatata zadacha kato 03.demo no napravena s do while
            int n = int.Parse(Console.ReadLine());
            int fac = 1;
            int i = 1;

            do
            {
                fac *= i;
                i++;
            } while (i <= n);

            Console.WriteLine("Fac on number {0} is {1}", n , fac);
        }
    }
}
