using System;

namespace _04.Demo
{
    class Program
    {
        static void Main(string[] args)
        {
            //Printira 4etnite stepeni na n 
            int n = int.Parse(Console.ReadLine());
            int number = 1;
            
            for (var i = 0; i <= n; i += 2)
            {
                Console.WriteLine(number);
                number = number * 2 * 2;
            }
        }
    }
}
