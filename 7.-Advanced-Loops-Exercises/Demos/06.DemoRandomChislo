using System;

namespace _10.Demo
{
    class Program
    {
        static void Main(string[] args)
        {
            //pechata random chislo i cikala spira kogato chislota e 7
            Random random = new Random();

            while (true)
            {
                int number = random.Next(1, 13);
                Console.WriteLine(number);

                if (number==7)
                {
                    break;
                }
            }
        }
    }
}
