using System;

namespace _06.Demo
{
    class Program
    {
        static void Main(string[] args)
        {
            //Zaa4a za nai golqm obsh delitel na 2 chisla

            int a = int.Parse(Console.ReadLine());
            int b = int.Parse(Console.ReadLine());

            while (b != 0)
            {
                int oldB = b;
                b = a % b;
                a = oldB;
            }

            Console.WriteLine(a);
        }
    }
}
