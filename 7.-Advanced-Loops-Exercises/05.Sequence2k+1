using System;

namespace _05.Sequence2k_1
{
    class Sequence
    {
        static void Main(string[] args)
        {
            int n = int.Parse(Console.ReadLine());
            int number = 1;

            for (int i = 1; i <= n; i++)
            {
                if (number <= n)
                {
                    Console.WriteLine(number);
                    number = (number * 2) + 1;
                }
            }
        }
    }
}
