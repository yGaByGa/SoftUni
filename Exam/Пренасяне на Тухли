using System;

namespace _05.Demo
{
    class Tyhli
    {
        static void Main(string[] args)
        {
            int n = int.Parse(Console.ReadLine()); // broi tuhli
            int w = int.Parse(Console.ReadLine()); // broi rabotnici
            int m = int.Parse(Console.ReadLine()); // broi tuhli na kurs

            double result = Math.Ceiling( (double)n / w / m );

            Console.WriteLine(result);
        }
    }
}
